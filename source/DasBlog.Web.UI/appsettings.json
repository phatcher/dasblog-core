{
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "IdentityOptions": {
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 8,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": true,
      "RequireLowercase": false,
      "RequiredUniqueChars": 6
    },
    "Lockout": {
      "DefaultLockoutTimeSpan": "00:30:00",
      "MaxFailedAccessAttempts": 10,
      "AllowedForNewUsers": true
    },
    "User": {
      "RequireUniqueEmail": true
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Microsoft": "Error",
      "DasBlog": "Information"
    }
  }
}